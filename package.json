{
  "name": "dnb",
  "version": "1.0.0",
  "description": "A multi-tenant SaaS for seafood sellers to manage products, buyers, and digital negotiations. Business owners onboard teams, add products, and create offers. Buyers respond via secure links with accept, reject, or counter. Offers are tracked, versioned, and analysed, with subscriptions and payments integrated.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx sequelize-cli db:migrate",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seed.js",
    "dev:backend": "npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
